{
  "name": "meathill/octopus",
  "description": "A PHP framework with UI created for API.",
  "keyword": [
    "framework",
    "ui",
    "RESTful"
  ],
  "homepage": "https://github.com/meathill/octopus-api",
  "license": "MIT",
  "author": [
    {
      "name": "Meathill",
      "email": "meathill@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/meathill/octopus-api/issues",
    "source": "https://github.com/meathill/octopus-api",
    "docs": "https://github.com/meathill/octopus-api/wiki"
  },
  "require": {
    "php": ">=5.6",
    "vlucas/phpdotenv": "^2.3"
  },
  "require-dev": {
    "monolog/monolog": "^1.21",
    "meathill/friday": "dev-master",
    "dianjoy/macaw": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Meathill\\Octopus\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "server-start": "cd server && php -S localhost:8888 ./routing.php"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
